<h1>All Volunteers</h1>

 <%= link_to 'Register a New Volunteer', new_user_path %>

<table>
  <tr>
    <th>Date Authorized as Volunteer</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Administrator</th>
    <th>Date Authorized as Administrator</th>
    <th>Email</th>
    <th>Phone Number</th>
    <th>Preferred Contact Method</th>
    <th>County</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
  </tr>
 
  <% @user.each do |user| %>
    <tr>
      <td><%= user.date_auth %></td>
      <td><%= user.first_name%></td>
      <td><%= user.last_name %></td>
      <td><%= user.admin %></td>
      <td><%= user.admin_auth %></td>
      <td><%= user.email %></td>
      <td><%= user.phone %></td>
      <td><%= user.contact_type %></td>
      <td><%= user.county %></td>
      <td><%= user.address1 %></td>
      <td><%= user.address2 %></td>
      <td><%= user.city %></td>
      <td><%= user.state %></td>
      <td><%= user.zip %></td><br>
      <td><%= link_to 'Show', user_path(user) %></td><br>
      <td><%= link_to 'Update', edit_user_path(user) %></td>
    </tr>
  <% end %>
</table>