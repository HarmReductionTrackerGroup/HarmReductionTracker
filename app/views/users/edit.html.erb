<h2>Edit </h2>

  <%= form_with model: @user, local: true do |f| %>

  
<div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </div>
<div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.phone_field :phone %>
  </div>
  <div class="field">
    <%= f.label :county %><br />
    <%= f.text_field :county %>
  </div>
  <div class="field">
    <%= f.label :address1 %><br />
    <%= f.text_field :address1 %>
  </div>
  <div class="field">
    <%= f.label :address2 %><br />
    <%= f.text_field :address2 %>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </div>
  <div class="field">
  <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </div>
  <div class="field">
  <%= f.label :admin %><br />
    <%= f.check_box :admin, {}, true, false %>
  </div>
  <div class="field">
  <%= f.label :contact_type %><br />
    <%= f.text_field :contact_type %>
  </div>
  <div class="field">
  <%= f.label :date_auth %><br />
    <%= f.date_field :date_auth %>
  </div>
  <%# <div class="field"> %>
  <%# <%= f.label :admin_auth %><br />
    <%# <%= f.date_field :admin_auth %>
  <%# </div> %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autocomplete: "email" %>
  </div>


  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>

<%= link_to "Back", :back %>