<h1>Inventory Kit List</h1>
 <%= link_to 'Enter a New Kit', new_inventory_path %>
<table>
  <tr>
    <th><%= sort_link "serial_number" %></th>
    <th><%= sort_link "kit_type" %></th>
    <th><%= sort_link "expiration_date" %></th>
    <th>Distributed Date </th>
    <th>Client ID </th>
    <th>User ID </th>
  </tr>
 
  <% @inventories.each do |inventory| %>
    <tr>
      <td><%= inventory.serial_num %></td>
      <td><%= inventory.kit_type%></td>
      <td><%= inventory.expiration_date %></td>
      <td><%= inventory.distributed_date %></td>
      <td><%= inventory.client_id %></td>
      <td><%= inventory.user_id %></td>
      <td><%= link_to 'Update', edit_inventory_path(inventory) %></td>
    </tr>
  <% end %>
</table>
  <%= paginate @inventories %>
  <%= page_entries_info @inventories %>
<br>

<%= link_to 'Back', users_path %>